<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>usuarioConsultaServlet</servlet-name>
        <servlet-class>servlet.usuarioConsultaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>articuloConsultaServlet</servlet-name>
        <servlet-class>servlet.articuloConsultaServlet</servlet-class>
    </servlet>
   
    <servlet-mapping>
        <servlet-name>usuarioConsultaServlet</servlet-name>
        <url-pattern>/usuarioConsultaServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>articuloConsultaServlet</servlet-name>
        <url-pattern>/articuloConsultaServlet</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
